<?xml version="1.0" encoding="utf-8"?>
<shape
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:shape="rectangle" >
    <corners
        android:radius="6dp" />
    <gradient
        android:angle="270"
        android:centerColor="#00B030"
        android:startColor="#008030"
        android:endColor="#006030"
        android:type="linear"/>
    <padding
        android:left="5dp"
        android:top="5dp"
        android:right="5dp"
        android:bottom="5dp" />
    <stroke
        android:width="1dp"
        android:color="#005421" />
</shape>